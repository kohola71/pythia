<%= form_for(@decision) do |f| %>
<div class = field> 
	<%= f.label :question %>
	<%= f.text_field :question %>
</div> 
<div class = field> 
	<%= f.label :description %>
	<%= f.text_field :description %>
</div> 
<div class = field> 
	<%= f.label :options %>
	<%= f.text_field :options %>
</div> 


<%= f.submit :Save %>
<% end %>

