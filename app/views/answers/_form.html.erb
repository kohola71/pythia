<%= form_for([@question, @question.answers.build]) do |f| %>
<p>
  <%= f.radio_button(:body, "YES") %>
  <%= f.label "YES" %>
  <%= f.radio_button(:body, "NO") %>
  <%= f.label "NO" %>
  <%= f.text_area(:body, size: "60x12") %>
  <%= f.label "Other" %>
  </p>


 
  <p>
    <%= f.submit %>
  </p>
<% end %>


